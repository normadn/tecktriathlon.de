{
  "name": "tecktriathlon.de",
  "version": "4.7.1",
  "description": "Website for tecktriathlon.de!",
  "author": "Norman Saade",
  "homepage": "https://github.com/amsel-tech/tecktriathlon.de",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amsel-tech/tecktriathlon.de"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "style": "eslint src/**/*.js pages/**/*.js",
    "format": "prettier --write 'src/**/*.js' 'pages/**/*.js' && eslint src/**/*.js pages/**/*.js --fix",
    "test": "npm run jest-tests && npm run cypress-tests",
    "jest-tests": "jest",
    "jest-tests-watch": "jest --watchAll",
    "jest-tests-with-coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cypress-tests": "start-server-and-test dev http://localhost:3000 cypress-run",
    "cypress-run": "cypress run",
    "cypress-open": "cypress open",
    "cypress-run-record": "cypress run --record",
    "cypress-verify": "cypress verify"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@sentry/nextjs": "^7.0.0",
    "moment-timezone": "^0.5.21",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^18.2.0",
    "react-responsive": "^9.0.0",
    "react-scrollspy": "^3.3.5",
    "react-slick": "^0.30.0",
    "react-transition-group": "^4.4.5",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.50.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "pretty-quick": "^4.0.0",
    "regenerator-runtime": "^0.14.0",
    "start-server-and-test": "^2.0.0"
  }
}
